{
  "name": "datproject.org",
  "version": "1.0.0",
  "description": "An online place for dats.",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/datproject/datproject.org.git"
  },
  "bugs": {
    "url": "https://github.com/datproject/datproject.org/issues"
  },
  "main": "server.js",
  "scripts": {
    "dev": "bankai client/app.js -c [ -u sheetify-nested ] -w --assets public",
    "start": "bankai build client/app.js public/ -c [ -u sheetify-nested ] && node ./server.js",
    "test": "standard && dependency-check ./package.json --entry client/app.js"
  },
  "devDependencies": {
    "bankai": "^8.1.1",
    "dependency-check": "^2.9.1",
    "standard": "^7.1.2"
  },
  "dependencies": {
    "choo": "^6.0.0",
    "compression": "^1.7.0",
    "dat-colors": "^3.4.0",
    "dat-icons": "^2.5.0",
    "express": "^4.15.4",
    "express-simple-redirect": "^1.0.1",
    "sheetify": "^6.1.1",
    "sheetify-nested": "^1.0.2",
    "tachyons": "^4.8.1"
  }
}
